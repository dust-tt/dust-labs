{
  "name": "Dust",
  "author": {
    "name": "Dust",
    "email": "support@dust.tt",
    "url": "https://dust.tt"
  },
  "domainWhitelist": ["dust.tt", "eu.dust.tt"],
  "parameters": [
    {
      "name": "dust_api_key",
      "type": "text",
      "secure": true,
      "required": true
    },
    {
      "name": "dust_workspace_id",
      "type": "text",
      "secure": true,
      "required": true
    },
    {
      "name": "region",
      "type": "text",
      "required": false,
      "default": "us",
      "helpText": "Optional: Set to 'eu' for EU region. Leave empty for default region."
    },
    {
      "name": "default_assistant_ids",
      "type": "multiline",
      "required": false
    },
    {
      "name": "hide_customer_information",
      "type": "checkbox",
      "default": false,
      "required": false
    }
  ],
  "defaultLocale": "en",
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html"
    }
  },
  "version": "1.1.0",
  "frameworkVersion": "2.0",
  "screenshots": [
    {
      "url": "assets/screenshot-0.png"
    },
    {
      "url": "assets/screenshot-1.png"
    },
    {
      "url": "assets/screenshot-2.png"
    }
  ]
}
