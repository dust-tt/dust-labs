# Freshservice Configuration
# Your Freshservice domain (e.g., "yourcompany" for yourcompany.freshservice.com)
FRESHSERVICE_DOMAIN=yourcompany

# Your Freshservice API key (found in Freshservice Admin > API Keys)
FRESHSERVICE_API_KEY=your_api_key_here

# Filter for tickets to sync (default: "updated_since=24h")
# Note: Only updated_since filter is supported in v2 API
# Examples:
# - "updated_since=24h" (tickets updated in last 24 hours)
# - "updated_since=1h" (tickets updated in last hour)
# - "updated_since=7d" (tickets updated in last 7 days)
# - "updated_since=2024-01-01T00:00:00Z" (tickets updated since specific ISO date)
FRESHSERVICE_FILTER=updated_since=24h

# Dust Configuration
# Your Dust API key (get from dust.tt workspace settings)
DUST_API_KEY=your_dust_api_key

# Optional: Custom Dust API base URL
# DUST_BASE_URL=endpoint

# Your Dust workspace ID
DUST_WORKSPACE_ID=your_workspace_id

# The space ID where tickets will be stored
DUST_SPACE_ID=your_space_id

# The datasource ID for Freshservice tickets
DUST_DATASOURCE_ID=freshservice-tickets

# Rate limiting configuration (optional)
DUST_RATE_LIMIT=120
FRESHSERVICE_MAX_CONCURRENT=2
FRESHSERVICE_RATE_LIMIT=1000 