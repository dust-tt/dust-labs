# Front API Configuration
FRONT_API_TOKEN=your_front_api_token

# Dust API Configuration
DUST_API_KEY=your_dust_api_key
DUST_WORKSPACE_ID=your_workspace_id
DUST_SPACE_ID=your_space_id
DUST_DATASOURCE_ID=your_datasource_id

# Optional: Filter conversations by inbox only
# Examples:
# FRONT_FILTER=inbox=inb_123  # Conversations from specific inbox ID (API format)
# FRONT_FILTER=inbox=30231326 # Conversations from specific inbox (URL format - auto-converted)

# Rate limiting configuration (optional)
# Default: 50 requests per minute
FRONT_RATE_LIMIT=50 