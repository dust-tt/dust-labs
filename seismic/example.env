# Seismic API credentials (OAuth2 Client Credentials)
SEISMIC_CLIENT_ID=your_seismic_client_id
SEISMIC_CLIENT_SECRET=your_seismic_client_secret
SEISMIC_TENANT=your_seismic_tenant
SEISMIC_DELEGATION_USER_ID=your_delegation_user_id

# Dust API credentials
DUST_API_KEY=your_dust_api_key
DUST_WORKSPACE_ID=your_dust_workspace_id
DUST_DATASOURCE_ID=your_dust_datasource_id
DUST_SPACE_ID=your_dust_space_id

# Dust API base URL (optional - defaults to US region)
# For EU region, use: https://eu.dust.tt/api/v1
# DUST_API_BASE_URL=https://eu.dust.tt/api/v1

# Content filtering options (all optional)
# Filter by content format (e.g., PDF, TXT)
# CONTENT_TYPE_FILTER=PDF

# Filter by creation date - only sync content created in the last X days
# DAYS_BACK_FILTER=30

# Rate limiting and concurrency (optional)
DUST_RATE_LIMIT_PER_MINUTE=120
SEISMIC_MAX_CONCURRENT=2 
SEISMIC_RATE_LIMIT_PER_MINUTE=60