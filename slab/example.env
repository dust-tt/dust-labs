# Slab API credentials
SLAB_API_TOKEN=your_slab_api_token
SLAB_DOMAIN=app.slab.com

# Dust API credentials
DUST_API_KEY=your_dust_api_key
DUST_WORKSPACE_ID=your_dust_workspace_id
DUST_SPACE_ID=your_dust_space_id
DUST_DATASOURCE_ID=your_dust_datasource_id

# Dust API base URL (optional - defaults to US region)
# For EU region, use: https://eu.dust.tt/api/v1
# DUST_API_BASE_URL=https://eu.dust.tt/api/v1

# Rate limiting (optional)
DUST_RATE_LIMIT_PER_MINUTE=120
SLAB_RATE_LIMIT_PER_MINUTE=120

# Checkpointing (optional)
# If provided, the script will resume from this cursor token (from GraphQL pagination)
# To resume after interruption, use the cursor value printed in the logs
# RESUME_CURSOR=your_cursor_token_here

